#include <iostream>
using namespace std;

int main()
{
   string sen;
   cin>>sen;
		int flag=0,count=0;
		for(int i=0;i<sen.length();i++){
		    flag=0;
		    for(int j=0;j<sen.length();j++){
		        if(i!=j && sen[i]==sen[j]){
		            flag=1;
		            break;
		        }
		    }
		    if(flag==0){
		        count++;
		    }
		}
		cout<<count;
}
